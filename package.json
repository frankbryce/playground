{
  "name": "deep-playground-prototype",
  "version": "2016.3.10",
  "description": "",
  "private": true,
  "watch": {
    "build-js": {
      "patterns": ["src"],
      "extensions": "ts",
      "quiet": false
    },
    "build-css": {
      "patterns": ["src"],
      "extensions": "css",
      "quiet": false
    },
    "build-html": {
      "patterns": ["src"],
      "extensions": "html",
      "quiet": false
    }
  },
  "scripts": {
    "clean": "rimraf dist",
    "start": "npm run serve-watch",
    "prep": "copyfiles analytics.js dist && concat node_modules/material-design-lite/material.min.js node_modules/seedrandom/seedrandom.min.js > dist/lib.js",
    "build-css": "concat node_modules/material-design-lite/material.min.css src/styles.css > dist/bundle.css",
    "watch-css": "npm-watch build-css",
    "build-html": "copyfiles -u 1 src/index.html dist",
    "watch-html": "npm-watch build-html",
    "build-js": "browserify src/playground.ts -p [tsify] > dist/bundle.js",
    "watch-js": "npm-watch build-js",
    "build": "npm run prep && npm run build-js && npm run build-css && npm run build-html",
    "watch": "npm run prep && concurrently \"npm run watch-js\" \"npm run watch-css\" \"npm run watch-html\"",
    "serve": "http-server -o -c-1 dist/",
    "serve-watch": "concurrently \"http-server -o -c-1 dist/\" \"npm run watch\""
  },
  "devDependencies": {
    "@types/d3": "^3.5.43",
    "@types/node": "^13.9.0",
    "concat": "^1.0.3",
    "concurrently": "3.1.0",
    "copyfiles": "1.0.0",
    "http-server": "^0.12.1",
    "rimraf": "2.5.4",
    "tsify": "^4.0.1",
    "typescript": "^2.9"
  },
  "dependencies": {
    "browserify": "^16.5.0",
    "d3": "^3.5.16",
    "material-design-lite": "^1.3.0",
    "npm-watch": "^0.6.0",
    "rwlock": "^5.0.0",
    "seedrandom": "^2.4.3"
  }
}
